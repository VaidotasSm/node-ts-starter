{
  "name": "node-ts-starter",
  "version": "0.1.0",
  "description": "Starter Project for Node, TypeScript, ESLint, Jest",
  "keywords": [
    "node",
    "starter",
    "typescript"
  ],
  "author": "vaidotas.sm",
  "license": "MIT",
  "homepage": "https://github.com/VaidotasSm/node-ts-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VaidotasSm/node-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/VaidotasSm/node-ts-starter/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "lint": "eslint src --ext .ts",
